package algo2.semana01.intro;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class BinaryCalculatorTest {

	@Test
	void testGetResultadoConOperadorBasico() {
		assertEquals(40, new BinaryCalculator(8,5,"*").getResultado());
		assertEquals(16, new BinaryCalculator(112,7,"/").getResultado());
		assertEquals(591, new BinaryCalculator(265,326,"+").getResultado());		
	}
	
	@Test
	void testGetResultadoConOperadorNoValido() {
		fail("Not yet implemented");
	}
	
	@Test
	void testGetResultadoConDivisionConCero() {
		fail("Not yet implemented");
	}
}
